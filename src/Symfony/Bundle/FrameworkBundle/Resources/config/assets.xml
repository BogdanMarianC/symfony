<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="templating.asset.packages" class="Symfony\Component\Asset\Packages">
            <argument /> <!-- default package -->
            <argument type="collection" /> <!-- named packages -->
        </service>

        <service id="templating.asset.package" class="Symfony\Component\Asset\RequestUrlPackage" abstract="true">
            <argument type="service" id="request_stack" />
            <argument /> <!-- base URLs -->
            <argument /> <!-- version -->
            <argument /> <!-- version format -->
        </service>

    </services>
</container>
